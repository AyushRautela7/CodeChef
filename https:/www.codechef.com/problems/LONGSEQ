#include<bits/stdc++.h>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        string str;
        cin>>str;
        int arr[2]={0};
        int len=str.length();
        for(int i=0;i<len;i++){
            if(str[i]=='1'){
                arr[1]++;
            } else{
                arr[0]++;
            }
        }
        if(arr[0]==1 || arr[1]==1){
            cout<<"Yes"<<endl;
        } else{
            cout<<"No"<<endl;
        }
    }
    return 0;
}
